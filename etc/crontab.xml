<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="extend_warranty">
        <job name="extend_warranty_sync_products_cronjob" instance="Extend\Warranty\Cron\SyncProducts" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="extend_warranty_create_warranty_contract_cronjob" instance="Extend\Warranty\Cron\CreateContracts" method="execute">
            <config_path>crontab/default/jobs/extend_warranty_create_warranty_contract/schedule/cron_expr</config_path>
        </job>
    </group>
</config>
